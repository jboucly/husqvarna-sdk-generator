{
    "name": "husqvarna-sdk-generator",
    "version": "1.0.0",
    "description": "Generator of SDK for Husqvarna APIs",
    "scripts": {
        "fetch:openapi": "zx ./scripts/fetchOpenApi.mjs",
        "sdk:generate": "openapi-generator-cli generate --generator-key typescript-sdk",
        "sdk:build": "zx ./scripts/build-sdk.mjs",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "dev": "tsx src/index.ts",
        "release": "release-it"
    },
    "keywords": [
        "husqvarna",
        "automower",
        "sdk",
        "generator",
        "openapi"
    ],
    "author": {
        "name": "Jboucly",
        "email": "contact@jboucly.fr"
    },
    "license": "MIT",
    "packageManager": "pnpm@10.17.1",
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.23.4",
        "@release-it/conventional-changelog": "^10.0.1",
        "@types/node": "^24.6.0",
        "chalk": "^5.6.2",
        "dotenv": "^17.2.2",
        "prettier": "^3.6.2",
        "release-it": "^19.0.5",
        "rimraf": "^6.0.1",
        "tsx": "^4.20.6",
        "zx": "^8.8.3"
    }
}
